<section> 
    <div class = "container-fluid"
        <div class = "row">
            <div class = "col-sm-4">
                <div class="table-responsive">
                    <table class="table table-bordered">
                        <thead>
                            <h1>Courses</h1>
                        </thead>
                        <tbody>
                            {{#each courses}}
                                <tr>
                                    <td height = "100" type="text">{{this}}</td>
                                </tr>
                            {{/each}}
                        <tbody>
                    </table>
                </div>
            </div>
            <div class = "col-sm-8">
                <div class = "container-fluid">
                    <div class="table-responsive">
                        <table class="table table-bordered">
                            <thead>
                                <h1>This Week</h1>
                            </thead>
                            <tbody>
                                {{#each reviews_this}}
                                    <tr>
                                        <td height="100">{{this.date}} {{this.course}}</td>
                                    </tr>
                                {{/each}}
                            <tbody>
                        </table>
                    </div>
                </div>
                    <div class="table-responsive">
                        <table class="table table-bordered">
                            <thead>
                                <h1>Next Week</h1>
                            </thead>
                            <tbody>
                                {{#each reviews_next}}
                                    <tr>
                                        <td height = "100"> {{this.date}} {{this.course}} </td>
                                    </tr>
                                {{/each}}
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>